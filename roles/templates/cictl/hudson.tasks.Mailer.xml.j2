<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl>
  <smtpAuthUsername>{{ admin_email }}</smtpAuthUsername>
  <smtpAuthPassword>{{ admin_email_password }}</smtpAuthPassword>
  <replyToAddress>{{ admin_email }}</replyToAddress>
  <smtpHost>smtp.aol.com</smtpHost>
  <useSsl>true</useSsl>
  <smtpPort>465</smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>